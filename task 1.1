package first;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.WebDriverWait;

public class task1_2 {
	
	public static void main(String[]aargs) throws InterruptedException
	{
		
		WebDriver driver= new ChromeDriver();
		

		driver.get("https://www.way2automation.com/way2auto_jquery/frames-and-windows.php");
		String windowHandler = driver.getWindowHandle();
		Thread.sleep(2000);
		
		driver.findElement(By.xpath("//a[@href='#example-1-tab-1']")).click();
		Thread.sleep(2000);
		driver.switchTo().frame(0);
		driver.findElement(By.xpath("/html/body/div/p/a")).click();
		Thread.sleep(2000);
		driver.switchTo().window(windowHandler);
		Thread.sleep(2000);
		
		driver.findElement(By.xpath("//a[@href=\"#example-1-tab-2\"]")).click();
		Thread.sleep(2000);
		
		driver.switchTo().frame(1);
		driver.findElement(By.xpath("/html/body/div/p/a")).click();
		Thread.sleep(2000);
	
		driver.switchTo().window(windowHandler);
		
		driver.findElement(By.xpath("//*[@id=\"wrapper\"]/div[1]/div[1]/div[1]/ul/li[3]/a")).click();
		driver.switchTo().frame(2);
		driver.findElement(By.xpath("/html/body/div/p/a")).click();
		Thread.sleep(2000);
		
		driver.switchTo().window(windowHandler);
		
		driver.findElement(By.xpath("/html/body/section/div[1]/div[1]/div[1]/ul/li[4]/a")).click();
		driver.switchTo().frame(3);
		Thread.sleep(2000);
		driver.findElement(By.xpath("/html/body/div/p/a")).click();
		Thread.sleep(2000);
	
		driver.quit();
		
		
	}

}
